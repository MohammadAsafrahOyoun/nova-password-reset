{
    "name": "mohammadasafrahoyoun/nova-password-reset",
    "description": "Reset password directly from nova account page",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mastani\\NovaPasswordReset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mastani\\NovaPasswordReset\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
